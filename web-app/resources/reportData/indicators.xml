            <excutableScript
                classDomaine="Equipment"
                followOperand="add"
                isDenominator="false"
                isDynamicFinder="false"
                isIntermidiateVariable="false"
                useCountFunction="true" >
select equ.code from Equipment as equ inner join equ.status as equipmentStatus where equipmentStatus.status='OPERATIONAL' or equipmentStatus.status='PARTIALYOPERATIONAL' or equipmentStatus.status='UNDERMAINTENANCE' and equ.obsolete='1' and equ.dataLocation='locationidentifier'
