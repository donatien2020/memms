    <!-- MANAGEMENT_MEDICAL_EQUIPEMENT -->

    <indicator
        categoryCode="MANAGEMENT_MEDICAL_EQUIPEMENT"
        indicatorCode="MANA_EQUIPMENT_SHARE_OF_OPERATIONAL_EQUIPMENT" >

        <name>
            SHARE OF OPERATIONAL EQUIPMENT
            <!--
Slide 7: (total number equipment with STATUS=“Operational”) 
/ (total number equipment with STATUS={“Operational”; “Partially operational”, “Under maintenance”})
            -->
        </name>

        <scriptFormula>

            <excutableScript
                classDomaine="Equipment"
                followOperand="add"
                isDenominator="false"
                isDynamicFinder="false"
                isIntermidiateVariable="false"
                useCountFunction="true" >
select equ.code from Equipment as equ inner join equ.status as equipmentStatus where equipmentStatus.status='INSTOCK' and equ.dataLocation='locationidentifier'
            </excutableScript>

            <excutableScript
                classDomaine="Equipment"
                followOperand="add"
                isDenominator="false"
                isDynamicFinder="false"
                isIntermidiateVariable="false"
                useCountFunction="true" >
select equ.code from Equipment as equ inner join equ.status as equipmentStatus where equipmentStatus.status='OPERATIONAL' and equ.dataLocation='locationidentifier'
            </excutableScript>

            <excutableScript
                classDomaine="Equipment"
                followOperand="add"
                isDenominator="true"
                isDynamicFinder="false"
                isIntermidiateVariable="false"
                useCountFunction="true" >
select equ.code from Equipment as equ inner join equ.status as equipmentStatus where equipmentStatus.status='INSTOCK' or equipmentStatus.status='OPERATIONAL' or equipmentStatus.status='PARTIALYOPERATIONAL' or equipmentStatus.status='UNDERMAINTENANCE' and equ.dataLocation='locationidentifier'
            </excutableScript>
        </scriptFormula>

        <type>
         Normal
        </type>
    </indicator>

