    <indicator
        categoryCode="MANAGEMENT_MEDICAL_EQUIPEMENT"
        indicatorCode="MANA_EQUIPMENT_SHARE_OF_EQUIPMENT__DONATED" >

        <name>

            <!-- slid 11: (total number equipment with STATUS={“Operational”; “Partially operational”, “Under maintenance”} and PURCHASER = {“Donor”})/(total number equipment with STATUS={“Operational”; “Partially operational”, “Under maintenance”}) -->
           SHARE OF EQUIPMENT DONATED
        </name>

        <scriptFormula>

            <excutableScript
                classDomaine="Equipment"
                followOperand="add"
                isDenominator="false"
                isDynamicFinder="false"
                isIntermidiateVariable="false"
                useCountFunction="true" >
select equ.code from Equipment as equ inner join equ.status as equipmentStatus where equipmentStatus.status='INSTOCK' or equipmentStatus.status='OPERATIONAL' or equipmentStatus.status='PARTIALYOPERATIONAL' or equipmentStatus.status='UNDERMAINTENANCE' and equ.purchaser='BYDONOR' and equ.dataLocation='locationidentifier'
